<Window x:Class="upravnikKT2.RoomEquipmentDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:upravnikKT2"
        mc:Ignorable="d"
        FontSize="18"
        Title="Spisak opreme" Height="350" Width="350" Loaded="Window_Loaded" PreviewKeyDown="Window_PreviewKeyDown" GotFocus="Window_GotFocus">
    <Grid>
        <ListView x:Name="lista">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Naziv" Width="150" DisplayMemberBinding="{Binding EquipmentName}"/>
                    <GridViewColumn Header="Kolicina" Width="150" DisplayMemberBinding="{Binding Equipment_Amount}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
